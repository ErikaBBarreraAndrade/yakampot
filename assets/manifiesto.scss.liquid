.manifiesto{
	.section-manifiesto-general{
    .title{
      font-family: "Raleway";
      color: #000;
      font-size: 18px;
      line-height: 21px;
      letter-spacing: 2px;
      text-transform: uppercase;
      width: 300px;
      margin: auto;
      @media (min-width: 576px) {
        width: auto;
      }
      @media (min-width: 992px) {
        font-size: 25px;
        line-height: 30px;
      }
    }
    .number{
      font-family: "Marvel-Bold";
      text-transform: uppercase;
      margin-top: 60px;
      @media (min-width: 992px) {
        font-size: 22px;
        line-height: 27px;
      }
    }
    .rte-settings{
      font-family: "Raleway-Medium";
      letter-spacing: 0.5px;
      font-size: 12px;
      line-height: 20px;
      @media (min-width: 992px) {
        font-family: "Circular Std Book";
        font-size: 13px;
        line-height: 20px;
      }
      strong{
        font-family: 'Raleway-Semibold Italic';
        @media (min-width: 992px) {
          font-family: 'Circular Std Bold';
        }
      }
    }
    .titulo-final{
      font-family: "Raleway-Light";
      font-size: 15px;
      line-height: 18px;
      letter-spacing: 1px;
      text-transform: uppercase;
      margin: 60px auto 30px;
      width: 280px;
      @media (min-width: 576px) {
        width: auto;
      }
      @media (min-width: 992px) {
        font-family: "Raleway";
        margin: 88px auto 40px;
      }
    }
	}
	.section-manifiesto-texto-1{
    padding: 90px 0;
    .rte-settings{
      width: 315px;
      margin: 0 auto;
      @media (min-width: 576px) {
        width: 435px;
      }
      @media (min-width: 992px) {
        width: 464px;
      }
      strong{
        font-family: 'Raleway-Semibold Italic';
        @media (min-width: 992px) {
          font-family: 'Circular Std Bold';
        }
      }
    }
	}
	.section-manifiesto-texto-2{
    color: #FFFFFF;
		.content-text-global{
      padding: 120px 30px;
      @media (min-width: 992px) {
        width: 465px;
        margin-left: auto;
        padding: 0;
        padding-right: 120px;
      }
      .number{
        margin-top: 0px;
      }
      .rte-settings{
        font-family: "Raleway-Light";
        letter-spacing: 0.5px;
        font-size: 12px;
        line-height: 14px;
        @media (min-width: 992px) {
          font-family: "Circular Std Book";
          font-size: 13px;
          line-height: 20px;
        }
        strong{
          font-family: 'Raleway-Semibold Italic';
          @media (min-width: 992px) {
            font-family: 'Circular Std Bold';
          }
        }
      }
      .titulo-final{
        font-family: "Raleway-Light";
        font-size: 15px;
        line-height: 18px;
        letter-spacing: 1px;
        text-transform: uppercase;
        margin: 40px 0px;
        width: 245px;
        @media (min-width: 576px) {
          width: auto;
        }
        @media (min-width: 992px) {
          font-family: "Raleway-Bold";
          margin: 40px auto 0px;
        }
      }
		}
	}
  .section-manifiesto-texto-3{
    padding: 90px 0;
    .rte-settings{
      width: 315px;
      margin: 0 auto;
      @media (min-width: 576px) {
        width: 435px;
      }
      @media (min-width: 992px) {
        width: 464px;
      }
      strong{
        font-family: 'Raleway-Semibold Italic';
        @media (min-width: 992px) {
          font-family: 'Circular Std Bold';
        }
      }
    }
	  .content-images-section{
      margin-top: 90px;
      @media (min-width: 992px) {
        margin-top: 150px;
      }
	  }
	  .content-image{
      padding: 0;
      @media (min-width: 992px) {
        padding: 0 15px;
      }
		  img{
        @media (min-width: 576px) {
         object-fit: cover;
	        object-position: top;
          height: 525px;
        }
        @media (min-width: 1280px) {
          object-fit: cover;
          object-position: top;
          height: 625px;
        }
        @media (min-width: 1500px) {
          height: 860px;
        }
		  }
	  }
	  .content-images-section_block_2{
      margin-top: 120px;
      @media (min-width: 576px) {
        margin-top: 120px;
      }
      @media (min-width: 1280px) {
        margin-top: 155px;
      }
      @media (min-width: 1500px) {
        margin-top: 200px;
      }
    }
	  .section_image_block_2{
		  .content-image_block_2:first-child{
        margin-top: 120px;
        @media (min-width: 576px) {
          margin-top: 120px;
        }
        @media (min-width: 1280px) {
          margin-top: 190px;
        }
			  @media (min-width: 1700px) {
          margin-top: 250px;
        }
		  }
	  }
  }
  .content-button{
    margin-top: 73px;
    @media (min-width: 992px) {
      margin-top: 120px;
    }
    @media (min-width: 1700px) {
      margin-top: 160px;
    }
    a{
      font-family: "Raleway-Medium";
      color: #000;
      font-size: 11px;
      line-height: 13px;
      padding: 2px !important;
      @media (min-width: 992px) {
        font-size: 12px;
        line-height: 14px;
	      letter-spacing: 2px;
      }
      span{
        border-bottom: 1px solid #000;
      }
      &:hover{
        background: transparent !important;
      }
      &:after{
        content: none !important;
      }
    }
  }
}