  }
}
/*== general ==*/
/*== header ==*/
.site-nav__expanded-item{
  @media (min-width: 1400px) {
    margin-right: 13px;
  }
  @media (min-width: 1800px) {
    margin-right: 40px;
  }
	&.MARCA{
		.navbar-links-full-width{
			li{
        width: 100%;
			}
		}
	}
}
@media(max-width: 767px){
  #header_logo .logo-rentina{
    max-width: 155px !important;
  }
}
#header{
  .position-fix{
    padding-right: 50px;
    @media (min-width: 768px) and (max-width: 1024px) and (orientation: portrait) {
      padding-right: 0px;
    }
    @media (min-width: 768px) and (max-width: 1024px) and (orientation: landscape) {
      padding-right: 0px;
    }
    @media(max-width: 767px){
      padding-right: 0px;
    }
  }
  .page-content{
    padding: 0px;
  }
  header{
    .site-nav{
      padding: 0;
      .active{
        a{
          border-bottom: 1px solid #000;
        }
      }
      .full-width{
        .dropdown-menu{
          width: 325px !important;
          height: 100vh;
          .menu-title{
            padding: 16px 0 0px;
            a{
              text-transform: initial;
              letter-spacing: 2px;
            }
          }
          .widget-inner{
            padding-left: 4vw;
            .nav-links{
              li{
                padding: 0px;
                a{
                  color: #FFF;
                  font-size: 13px;
                  letter-spacing: 2px;
                }
              }
            }
          }
        }
      }
      li:nth-child(1){
        a{
          margin-left: 0;
        }
      }
      li{
        a{
          font-family: $font-medium;
          font-weight: 100;
          padding-right: 0;
          padding-left: 0;
          margin: 0px 22px;
        }
      }
    }
    #apollo-top-menu{
      .megamenu{
        padding-left: 50px;
      }
    }
  }
  #search-icon{
    font-family: $font-medium;
  }
  .header_user_info{
    font-family: $font-medium;
  }
  .mini-cart{
    a{
      font-family: $font-medium;
    }
    #CartCount{
      &:before{
        content: '(';
      }
      &:after{
        content: ')';
      }
    }
  }
  .whishlist-head {
    a{
      font-family: $font-medium;
    }
    #totle-product-wishlist{
      &:before{
        content: '(';
      }
      &:after{
        content: ')';
      }
    }
  }
}
.header-layout{
  position: relative;
  z-index: 9;
}

