/*== header responsive ==*/
/*== index ==*/
.template-index{
  .index-section-slider{
    .slick-dots{
      bottom: 80px!important;
      margin: 0 !important;
      @media(min-width: 768px){
        bottom: 60px!important;
      }
      @media(min-width: 1800px){
        bottom: 80px!important;
      }
      li{
        width: 11px;
        height: 11px;
        margin-left: 8px;
        border: 1px solid #FFF;
        border-radius: 50%;
        button{
          &:before{
            content: none;
          }
        }
        &.slick-active{
          background: #FFF;
        }
      }
    }
  }
  .slideshow{
    .slideshow__text-content{
      top: 70% !important;
    }
    .slideshow__title{
      font-family: $font-marvel;
      font-size: 55px;
      margin-bottom: 0px;
      @media(max-width: 900px){
        font-size: 45px;
      }
    }
  }
  .product-image-container{
    .functional-buttons,{
      display: none;
    }
  }
  .product-container{
    .review{
      display: none;
    }
  }
  .content-banner-index{
    display: -webkit-box;
    display: -webkit-flex;
    display: -ms-flexbox;
    display: flex;
    flex-wrap: wrap;
    .text-box{
      position: absolute;
      bottom: 10%;
      width: 100%;
      left: -15px;
      padding: 20px;
      .title{
        font-family: $font-marvel;
        font-size: 4vw;
        margin-bottom: 0px;
        @media(max-width: 900px){
          font-size: 7vw;
          color: #FFF;
        }
      }
      .rte-setting{
        font-family: $font-medium;
        font-size: 1.6vw;
        @media(max-width: 900px){
          font-size: 3vw;
        }
        p{
          color: #000;
          @media(max-width: 900px){
            color: #FFF;
          }
        }
      }
    }
  }
  .content-banner-index [class*='col-']{
    display: flex;
    flex-direction: column;
    background: #E5E3DF;
    padding: 0px;
  }
  .image-bar--x-large{
    .no-padding{
      padding-right: 10px !important;
    }
    .image-bar__content{
      @media screen and (min-width: 992px){
        height: 1230px;
      }
    }
  }
  .section-only-text{
    .hero__inner{
      .mega-subtitle{
        max-width: 100%;
      }
    }
    .hero__overlay{
      &::before{
        background-color: transparent;
      }
    }
    .subtitle{
      font-family: $font-medium;
      letter-spacing: 1px;
    }
    .hero__btn{
      margin-top: 25px;
    }
  }
  .banner-second-index{
    .hero--x-large{
      @media screen and (min-width: 992px){
        height: 1015px;
      }
    }
    .mega-title{
      font-family: $font-marvel;
      margin-bottom: 0px;
      font-size: 55px;
      @media(max-width: 900px){
        font-size: 7vw;
        color: #FFF !important;
      }
    }
    .hero__btn{
      white-space: inherit !important;
      margin-top: 50px;
    }
    .rte-setting{
      font-size: 20px;
      font-family: $font-medium;
      @media(max-width: 900px){
        font-size: 3vw;
        color: #FFF !important;
      }
      p{
        @media(max-width: 900px){
          color: #FFF !important;
        }
      }
    }
  }
  .btn-text-only{
    background: transparent;
    color: #000;
    border: none;
    text-transform: initial;
  }
}
/*== index ==*/
/*== aboutus ==*/
